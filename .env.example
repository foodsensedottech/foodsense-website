# App
NEXT_PUBLIC_BASE_URL=http://localhost:3000

# Analytics (Production Only)
NEXT_PUBLIC_GA_ID=
NEXT_PUBLIC_ENABLE_ANALYTICS=false

# Authentication
NEXTAUTH_URL=http://localhost:3000
